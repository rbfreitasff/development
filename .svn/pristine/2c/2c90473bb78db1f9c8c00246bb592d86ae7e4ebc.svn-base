'         sEmpresaNFCe = LerArquivoINI("NFe", "Empresa", CaminhoINI & "\System.ini")
         FileCopy I_UnidadeNFe & "NFC-e\Notas\Notas.TXT", I_UnidadeNFe & "NFC-e\" & I_EmpresaNFe & "\Envio\" & Trim(rsNFCe!ChaveNFCe) & "-env-canc.xml"
         Kill I_UnidadeNFe & "NFC-e\Notas\Notas.TXT"
      End If
   End If

End Function

Public Function FInutilizarNumero(ByVal iNumero As Integer) As String
Dim rsVerificar As New ADODB.Recordset
Dim rsUFs As New ADODB.Recordset
'Dim sEmpresaNFCe As String

   Set rsVerificar = cnSistema.Execute("SELECT * FROM NFCeInutilizadas WHERE Numero = " & iNumero)
   If rsVerificar.EOF Then
      Set rsUFs = cnSistema.Execute("SELECT * FROM UFs WHERE idUF = " & I_idEmpresaUF)

      Open I_UnidadeNFe & "NFC-e\Notas\Notas.TXT" For Output As #1

      Print #1, "tpAmb|" & LerArquivoINI("NFe", "Ambiente", CaminhoINI & "\System.ini") ' 1 - Produção ou 2 - Homologação
      Print #1, "versao|3.10"
      Print #1, "cUF|" & rsUFs!Codigo
      Print #1, "ano|" & Format(Date, "yy")
      Print #1, "CNPJ|" & RemoveCaracteres(I_Empresa_CNPJ_CPF)
      Print #1, "mod|55"
      Print #1, "serie|1"
      Print #1, "nNFIni|" & Trim(iNumero)
      Print #1, "nNFFin|" & Trim(iNumero)
      Print #1, "xJust|Numero inutilizado por escolha do usuario"

      Close #1

'      sEmpresaNFCe = LerArquivoINI("NFe", "Empresa", CaminhoINI & "\System.ini")
      FileCopy I_UnidadeNFe & "NFC-e\Notas\Notas.TXT", I_UnidadeNFe & "NFC-e\" & I_EmpresaNFe & "\Envio\" & StrZero(Val(iNumero), 12) & "-ped-inu.txt"
      Kill I_UnidadeNFe & "NFC-e\Notas\Notas.TXT"

      cnSistema.Execute "Insert Into NFCeInutilizadas (ChaveNFCe,Numero,Data,Protocolo) " & _
                        "Values ('','" & iNumero & "','" & Date & "','')"
                        
      FInutilizarNumero = "Número enviado para ser inutilizado"
   Else
      FInutilizarNumero = "Número já Inutilizado"
   End If

End Function

Public Function FConsultarNumero(ByVal iNumero As Integer) As String
Dim rsVerificar As New ADODB.Recordset
Dim rsUFs As New ADODB.Recordset
'Dim sEmpresaNFCe As String
Dim sChaveNFe As String
Dim sVerArquivo As String

   Set rsVerificar = cnSistema.Execute("SELECT * FROM NFCe WHERE Numero = " & iNumero)
   If Not rsVerificar.EOF Then
      sChaveNFe = rsVerificar!ChaveNFCe
      Set rsUFs = cnSistema.Execute("SELECT * FROM UFs WHERE idUF = " & I_idEmpresaUF)

      ' Copiar XML para ser Reprocessado
      sVerArquivo = Dir(I_UnidadeNFe & "NFC-e\" & I_EmpresaNFe & "\Erros\" & sChaveNFe & "-nfe.xml")
      If sVerArquivo = (sChaveNFe & "-nfe.xml") Then
         FileCopy I_UnidadeNFe & "NFC-e\" & I_EmpresaNFe & "\Erros\" & sChaveNFe & "-nfe.xml", I_UnidadeNFe & "NFC-e\" & I_EmpresaNFe & "\Enviados\EmProcessamento\" & sChaveNFe & "-nfe.xml"
      End If

      ' Envia solicitação de consulta
      Open I_UnidadeNFe & "NFC-e\Notas\Notas.TXT" For Output As #1
      
      Print #1, "<?xml version=" & Chr(34) & "1.0" & Chr(34) & " encoding=" & Chr(34) & "UTF-8" & Chr(34) & "?>"
      Print #1, "<consSitNFe xmlns=" & Chr(34) & "http://www.portalfiscal.inf.br/nfe" & Chr(34) & " versao=" & Chr(34) & "3.10" & Chr(34) & ">"
      Print #1, "<tpAmb>" & LerArquivoINI("NFe", "Ambiente", CaminhoINI & "\System.ini") & "</tpAmb>"   ' 1 - Produção ou 2 - Homologação
      Print #1, "<xServ>CONSULTAR</xServ> "
      Print #1, "<tpEmis>1</tpEmis>"
      Print #1, "<chNFe>" & sChaveNFe & "</chNFe>"
      Print #1, "</consSitNFe>"
      Close #1
      
'      sEmpresaNFCe = LerArquivoINI("NFe", "Empresa", CaminhoINI & "\System.ini")
      FileCopy I_UnidadeNFe & "NFC-e\Notas\Notas.TXT", I_UnidadeNFe & "NFC-e\" & I_EmpresaNFe & "\Envio\" & Trim(rsVerificar!ChaveNFCe) & "-ped-sit.xml"
      Kill I_UnidadeNFe & "NFC-e\Notas\Notas.TXT"
                        
'      FConsultarNumero = "Número enviado para ser inutilizado"
   Else
'      FConsultarNumero = "Número já Inutilizado"
   End If

End Function

