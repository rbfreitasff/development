   ' Criar Estrutura do Transportador
   '------------------------------------------------------------------------------------------
    With rsNFsTransportes
      .Fields.Append "nNF", adVarChar, 9                          ' Número da Nota
      .Fields.Append "modFrete", adVarChar, 1                     ' Modalidade do Frete
